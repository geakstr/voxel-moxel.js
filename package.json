{
  "name": "voxel-moxel.js",
  "version": "0.1.0",
  "description": "just for fun voxel game like minecraft written on js",
  "main": "lib/index.js",
  "repository": "https://github.com/geakstr/voxel-moxel.js",
  "author": "geakstr <geakstr@me.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "exit 0",
    "start": "concurrently \"yarn start-planet-worker\" \"yarn start-app\"",
    "start-app": "NODE_ENV=development node ./dev-server.js",
    "start-planet-worker":
      "NODE_ENV=development webpack --watch --colors --config ./configs/webpack.planet.worker.development.js",
    "build-planet-worker":
      "NODE_ENV=production webpack --colors --config ./configs/webpack.planet.worker.production.js",
    "build-app":
      "rm -rf ./lib/* && NODE_ENV=production webpack --colors --config ./configs/webpack.app.production.js",
    "build": "yarn build-app && yarn build-planet-worker",
    "deploy": "yarn build && yarn surge",
    "surge": "surge --project ./lib --domain voxel-moxel.dkharitonov.me"
  },
  "devDependencies": {
    "@types/gl-matrix": "2.4.0",
    "@types/is": "0.0.18",
