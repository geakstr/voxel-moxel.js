    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "brotli-webpack-plugin": "0.5.0",
    "compression-webpack-plugin": "1.1.3",
    "concurrently": "3.5.1",
    "copy-webpack-plugin": "4.3.1",
